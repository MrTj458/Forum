{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 80,
        "semi": false
      }
    ],
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-underscore-dangle": "off",
    "global-require": "off",
    "no-restricted-globals": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
